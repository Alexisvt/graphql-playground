{
  "name": "basic-relay",
  "version": "1.0.0",
  "description": "basic sample using relay graphql and react",
  "main": "src/server.js",
  "author": "Alexis Villegas Torres",
  "scripts": {
    "start": "npm-run-all --parallel serve watch",
    "serve": "babel-watch src/server.js",
