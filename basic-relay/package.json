{
  "name": "basic-relay",
  "version": "1.0.0",
  "description": "basic sample using relay graphql and react",
  "main": "src/server.js",
  "author": "Alexis Villegas Torres",
  "config": {
    "domain": "localhost",
    "PORT": 4000
  },
  "scripts": {
    "start": "npm-run-all --parallel serve watch",
    "serve": "cross-var \"babel-watch $npm_package_main\" ",
    "watch": "cross-var \" onchange \"./$npm_package_main\"  -- npm run open \" ",
    "open": "cross-var \"opn http://$npm_package_config_domain:$npm_package_config_PORT -- \"Chrome\" --incognito\" ",
    "db:populate": "babel-node data/loadTestMongoData.js"
  },
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "ejs": "^2.5.6",
    "express": "^4.14.1",
    "express-graphql": "^0.6.3",
    "graphql": "^0.9.1",
    "graphql-relay": "^0.5.1",
    "mongodb": "^2.2.24",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-relay": "^0.10.0",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.0.35",
    "@types/graphql": "^0.8.6",
    "@types/mongodb": "^2.1.40",
    "babel-eslint": "^7.1.1",
    "babel-plugin-react-relay": "^0.10.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-relay-plugin": "^0.11.0",
    "babel-watch": "^2.0.6",
    "cross-env": "^3.1.4",
    "cross-var": "^1.0.2",
    "eslint": "^3.16.0",
    "eslint-plugin-graphql": "^0.6.0",
    "eslint-plugin-react": "^6.10.0",
    "npm-run-all": "^4.0.1",
    "onchange": "^3.2.1",
    "opn-cli": "^3.1.0",
    "webpack-plugin-graphql-schema-hot": "^0.0.5"
  }
}
