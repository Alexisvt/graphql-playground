    "graphql": "^0.9.1",
    "graphql-relay": "^0.5.1",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.24",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-relay": "^0.10.0",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "@playlyfe/gql": "^2.2.1",
